      <region source="menu_skin_bitmap" hplacement="right" vplacement="stretch" x="32" y="10" w="10" h="1" />
      <region source="menu_skin_bitmap" hplacement="left" vplacement="bottom" x="21" y="11" w="10" h="10" />
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="bottom" x="31" y="11" w="1" h="10" />
      <region source="menu_skin_bitmap" hplacement="right" vplacement="bottom" x="32" y="11" w="10" h="10" />
      <text font="menu_font" hplacement="center" vplacement="center" color="#EFEFEF" />
    </frame>

    <frame name="progress.bar">
      <region source="menu_skin_bitmap" hplacement="left" vplacement="top" x="21" y="0" w="10" h="10" />
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="top" x="31" y="0" w="1" h="10" />
      <region source="menu_skin_bitmap" hplacement="left" vplacement="stretch" x="21" y="10" w="10" h="1" />
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="stretch" x="31" y="10" w="1" h="1" />
      <region source="menu_skin_bitmap" hplacement="left" vplacement="bottom" x="21" y="11" w="10" h="10" />
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="bottom" x="31" y="11" w="1" h="10" />
      <text font="menu_font" hplacement="center" vplacement="center" color="#FFF000" />
    </frame>

    <frame name="gridcell.normal">
      <region source="grid" hplacement="stretch" vplacement="stretch" x="0" y="0" w="50" h="50" />
    </frame>

    <frame name="gridcell.selectable">
      <region source="grid" hplacement="stretch" vplacement="stretch" x="0" y="50" w="50" h="50" />
    </frame>

    <frame name="gridcell.targetted">
      <region source="grid" hplacement="stretch" vplacement="stretch" x="0" y="100" w="50" h="50" />
    </frame>

    <frame name="gridcell.splashed">
      <region source="grid" hplacement="stretch" vplacement="stretch" x="0" y="150" w="50" h="50" />
    </frame>

    <frame name="label">
      <text font="menu_font" hplacement="left" vplacement="top" color="#FFFFFF" />
    </frame>

    <frame name="window">
      <region source="menu_skin_bitmap" hplacement="left" vplacement="top" x="0" y="0" w="10" h="10" />
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="top" x="10" y="0" w="1" h="10" />
      <region source="menu_skin_bitmap" hplacement="right" vplacement="top" x="11" y="0" w="10" h="10" />
      <region source="menu_skin_bitmap" hplacement="left" vplacement="stretch" x="0" y="10" w="10" h="1" />
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="stretch" x="10" y="10" w="1" h="1" />
      <region source="menu_skin_bitmap" hplacement="right" vplacement="stretch" x="11" y="10" w="10" h="1" />
