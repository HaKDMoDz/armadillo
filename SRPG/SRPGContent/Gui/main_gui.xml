      <region source="menu_skin_bitmap" hplacement="right" vplacement="bottom" x="11" y="11" w="10" h="10" />
    </frame>

    <frame name="radialbuttoncontrol.normal">
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="stretch" x="0" y="21" w="64" h="64" />
    </frame>

    <frame name="radialbuttoncontrol.highlighted">
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="stretch" x="64" y="21" w="64" h="64" />
    </frame>

    <frame name="radialbuttoncontrol.depressed">
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="stretch" x="0" y="85" w="64" h="64" />
    </frame>
	
	<frame name="radialbuttoncontrol.disabled">
      <region source="menu_skin_bitmap" hplacement="stretch" vplacement="stretch" x="64" y="85" w="64" h="64" />
    </frame>

    <frame name="radialcontrol.images.attack.normal">
      <region source="battle_radial_bitmap" hplacement="stretch" vplacement="stretch" x="0" y="0" w="64" h="64"/>
    </frame>
	<frame name="radialcontrol.images.attack.highlighted">
      <region source="battle_radial_bitmap" hplacement="stretch" vplacement="stretch" x="64" y="0" w="64" h="64"/>
    </frame>
	<frame name="radialcontrol.images.attack.depressed">
      <region source="battle_radial_bitmap" hplacement="stretch" vplacement="stretch" x="128" y="0" w="64" h="64"/>
    </frame>
	<frame name="radialcontrol.images.attack.disabled">
